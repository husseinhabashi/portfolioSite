#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules/update/bin/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules/update/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules/update/bin/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules/update/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/update@0.7.4/node_modules:/workspaces/portfolioSite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/update@0.7.4/node_modules/update/bin/update.js" "$@"
else
  exec node  "$basedir/../.pnpm/update@0.7.4/node_modules/update/bin/update.js" "$@"
fi
